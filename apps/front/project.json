{
  "name": "front",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/front",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project front --web",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/front",
        "command": "next build"
      },
      "dependsOn": [
        "^build"
      ],
      "cache": true,
      "inputs": [
        "default",
        "^production",
        {
          "externalDependencies": [
            "next"
          ]
        }
      ],
      "outputs": [
        "{workspaceRoot}/apps/front/.next",
        "{workspaceRoot}/apps/front/.next/!(cache)"
      ],
      "parallelism": true
    }
  }
}
